{
  "name": "frontend-factory",
  "packageManager": "yarn@4.4.0",
  "scripts": {
    "dev:admin": "yarn workspace jsonlee-admin dev",
    "commit": "git-cz",
    "lint": "eslint .",
    "fix": "eslint --fix ."
  },
